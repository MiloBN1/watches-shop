{
  "version": 2,
  "builds": [
    {
      "src": "server/watches-server/src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/watches-server/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/watches-shop/$1"
    }
  ]
}
